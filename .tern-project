{
  "ecmaVersion": 7,
  "libs": [
    "chai"
  ],
  "loadEagerly": [
    "lib/**/*.js"
  ],
  "plugins": {
    "doc_comment": {
      "fullDocs": true,
      "strong": false
    },
    "node": {
      "dontLoad": "",
      "load": "",
      "modules": ""
    },
    "node_resolve": {},
    "modules": {
      "dontLoad": "",
      "load": "",
      "modules": ""
    },
    "es_modules": {},
    "commonjs": {}
  }
}